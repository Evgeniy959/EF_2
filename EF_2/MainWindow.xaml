<Window x:Class="EF_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EF_2"
        mc:Ignorable="d"
        Title="Тест БД" Height="222" Width="143">
    <StackPanel>
        <Label>Логин</Label>
        <TextBox x:Name="loginTB" Margin="8"></TextBox>
        <Label>Пароль</Label>
        <TextBox x:Name="passTB" Margin="8"></TextBox>
        <Button Margin="8" Click="Button_Click">Вход</Button>
        <Button Margin="8" Click="Button_Click">Регистрация</Button>

    </StackPanel>
</Window>
